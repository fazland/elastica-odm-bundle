<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="Fazland\ODM\ElasticaBundle\Command\DropSchemaCommand" class="Fazland\ODM\ElasticaBundle\Command\DropSchemaCommand">
            <argument type="service" id="fazland_elastica_odm.document_manager" />
            <tag name="console.command" />
        </service>

        <service id="Fazland\ODM\ElasticaBundle\Command\UpdateSchemaCommand" class="Fazland\ODM\ElasticaBundle\Command\UpdateSchemaCommand">
            <argument type="service" id="fazland_elastica_odm.document_manager" />
            <tag name="console.command" />
        </service>
    </services>
</container>
