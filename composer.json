{
    "name": "fazland/elastica-odm-bundle",
    "description": "Elastica ODM Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Massimiliano Braglia",
            "email": "massimiliano.braglia@fazland.com"
        },
        {
            "name": "Alessandro Chitolina",
            "email": "alessandro.chitolina@fazland.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "fazland/elastica-odm": "^1.0",
        "symfony/config": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/framework-bundle": "^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpunit/phpunit": "^7.0",
        "symfony/cache": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Fazland\\ODM\\ElasticaBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "archive": {
        "exclude": ["tests", "vendor", ".php_cs", ".gitignore", "phpunit.xml.dist", "README.md"]
    }
}
